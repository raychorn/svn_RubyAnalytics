<h1>Listing query_results</h1>

<table>
  <tr>
    <th>Data source</th>
    <th>Query string</th>
    <th>Store in db</th>
    <th>Latest Results</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @query_results.each do |query_result| %>
  <tr>
    <td><%= query_result.data_source_id %></td>
    <td><%= query_result.query_string %></td>
    <td><%= query_result.store_in_db %></td>
    <td><%= query_result.latest_data_results %></td>
    <td><%= link_to 'Show', query_result %></td>
    <td><%= link_to 'Edit', edit_query_result_path(query_result) %></td>
    <td><%= link_to 'Destroy', query_result, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Query result', new_query_result_path %>
